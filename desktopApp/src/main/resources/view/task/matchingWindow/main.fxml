<?xml version="1.0" encoding="UTF-8"?>


<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<?import kurlyk.view.common.component.DraggingListView?>
<?import kurlyk.view.common.component.MyHtmlEditor?>
<VBox xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="kurlyk.view.task.matchingWindow.MatchingController">
    <HBox>
        <MyHtmlEditor fx:id="textArea" HBox.hgrow="ALWAYS">

        </MyHtmlEditor>
    </HBox>
    <VBox fx:id="root" alignment="TOP_CENTER">
        <HBox>
            <DraggingListView fx:id="leftField" HBox.hgrow="ALWAYS"/>
            <DraggingListView fx:id="rightField" HBox.hgrow="ALWAYS"/>
        </HBox>
    </VBox>
    <HBox alignment="CENTER" HBox.hgrow="ALWAYS">
        <Button fx:id="submit" text="Ответ">
            <font>
                <Font size="15.0" />
            </font>
        </Button>
    </HBox>
</VBox>
